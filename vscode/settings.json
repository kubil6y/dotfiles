{
    "editor.fontFamily": "MonoLisa Static",
    "editor.suggestSelection": "first",
    "editor.renderLineHighlight": "none",
    "editor.lightbulb.enabled": false,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.hideCursorInOverviewRuler": true,
    "editor.scrollbar.vertical": "hidden",
    "editor.codeLens": false,
    "editor.guides.indentation": false,
    "editor.renderWhitespace": "none",
    "editor.matchBrackets": "near",
    "explorer.compactFolders": false,
    "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
    "files.restoreUndoStack": false,
    "files.associations": {
        "*.html": "html"
    },
    "files.exclude": {
        ".godot/": true,
        "**/*.meta": true,
        "**/*.o": true,
        "**/obj": true
    },
    "go.toolsManagement.autoUpdate": true,
    // "omnisharp.enableImportCompletion": true,
    // "omnisharp.useEditorFormattingSettings": true,
    "omnisharp.organizeImportsOnFormat": true,
    // .net:true, unity:false
    "omnisharp.useModernNet": false,
    "omnisharp.monoPath": "/usr/bin/mono",
    "omnisharp.enableAsyncCompletion": true,
    "security.workspace.trust.enabled": false,
    "terminal.integrated.defaultProfile.linux": "bash",
    "terminal.integrated.persistentSessionReviveProcess": "never",
    // "workbench.editor.enablePreview": false,
    "workbench.layoutControl.enabled": false,
    "workbench.sideBar.location": "left",
    "workbench.tree.indent": 12,
    "workbench.editor.restoreViewState": false,
    "workbench.startupEditor": "none",
    "workbench.iconTheme": "material-icon-theme",
    "editor.tokenColorCustomizations": {
        "[Default Dark+ Experimental]": {
            "variables": "#FFF1F1F1",
            "textMateRules": [
                {
                    "scope": [
                        "variable.other.constant.object.ts",
                        "variable.other.constant.object.js",
                        "variable.other.constant.ts",
                        "meta.definition.variable.ts",
                        "meta.var-single-variable.expr.ts",
                        "meta.var.expr.ts",
                        "source.ts",
                        "support.type.property-name",
                        "variable.other.enummember"
                    ],
                    "settings": {
                        "foreground": "#FFF1F1F1"
                    }
                },
                {
                    "scope": "emphasis",
                    "settings": {
                        "fontStyle": ""
                    }
                }
            ]
        },
        "[Default Dark+]": {
            // D4D4D4,FFF1F1F1,e2e2e2
            "variables": "#FFF1F1F1",
            "textMateRules": [
                {
                    "scope": [
                        "variable.other.constant.object.ts",
                        "variable.other.constant.object.js",
                        "variable.other.constant.ts",
                        "meta.definition.variable.ts",
                        "meta.var-single-variable.expr.ts",
                        "meta.var.expr.ts",
                        "source.ts",
                        "support.type.property-name",
                        "variable.other.enummember"
                    ],
                    "settings": {
                        "foreground": "#FFF1F1F1"
                    }
                },
                {
                    "scope": "keyword.preprocessor.cs",
                    "settings": {
                        "foreground": "#808080"
                    }
                }
            ]
        },
        "[Monokai Pro (Filter Ristretto)]": {
            "textMateRules": [
                {
                    "scope": [
                        "storage.type",
                        "variable.parameter",
                        "parameters variable.function",
                        "support.type",
                        "storage.modifier",
                        "entity.other.inherited-class",
                        "entity.other.attribute-name",
                        "comment",
                        "punctuation.definition.comment",
                        "invalid"
                    ],
                    "settings": {
                        "fontStyle": ""
                    }
                }
            ]
        },
        "[Gruvbox Dark (Hard)]": {
            "textMateRules": [
                {
                    "scope": [
                        "entity.name.class.static.function",
                        "entity.name.function"
                    ],
                    "settings": {
                        "fontStyle": "bold"
                    }
                },
                {
                    "scope": ["comment", "punctuation.definition.comment"],
                    "settings": {
                        "fontStyle": ""
                    }
                }
            ]
        }
    },
    "workbench.colorCustomizations": {
        "[Default Dark+ Experimental]": {
            "statusBar.background": "#1f1f1f",
            "statusBar.border": "#1f1f1f",
            "editorLineNumber.foreground": "#6e7681",
            "editorLineNumber.activeForeground": "#6e7681"
        },
        "[Default Dark+]": {
            "activityBar.background": "#1e1e1e",
            "statusBar.background": "#1e1e1e",
            "editorOverviewRuler.border": "#1e1e1e",
            "editorLineNumber.foreground": "#6e7681",
            "editorLineNumber.activeForeground": "#6e7681",
            "tab.inactiveBackground": "#1e1e1e",
            "tab.activeBorderTop": "#0078d4",
            "editorGroupHeader.tabsBackground": "#1e1e1e"
        },
        "[Monokai Pro (Filter Ristretto)]": {
            "statusBar.background": "#2c2525",
            "editorGroup.emptyBackground": "#2c2525",
            "activityBar.background": "#2c2525",

            "tab.activeBorderTop": "#f9cc6c",
            "tab.activeBorder": "#2c2525",
            "tab.unfocusedActiveBorder": "#2c2525",
            "tab.unfocusedActiveBorderTop": "#948a8b",

            "editorHoverWidget.highlightForeground": "#ffd866",
            "editorLineNumber.activeForeground": "#5b5353",

            "editorSuggestWidget.selectedBackground": "#505050"
        },
        "[Ros√© Pine (no italics)]": {
            "list.highlightForeground": "#e0def4",
            "list.focusHighlightForeground": "#e0def4",
            "editorSuggestWidget.focusHighlightForeground": "#e0def4",
            "editorSuggestWidget.highlightForeground": "#e0def4",
            "editorLineNumber.activeForeground": "#908caa",
            "editorCursor.background": "#191724",
            "editorCursor.foreground": "#e0def4",
            "terminal.ansiBrightBlue": "#31748f",
            "tab.activeBorderTop": "#31748f"
        },
        "[Gruvbox Dark (Hard)]": {
            // "editorError.foreground": "#d79921",
            "editorInlayHint.foreground": "#868686f0",
            "editorInlayHint.background": "#18181800",
            // Overrides for specific kinds of inlay hints
            "editorInlayHint.typeForeground": "#486148f0",
            "editorInlayHint.parameterForeground": "#486148f0",

            "activityBar.background": "#1d2021",
            "statusBar.background": "#1d2021", //282828,1d2021
            "statusBar.foreground": "#858585",
            "tab.activeBorderTop": "#d79921",
            "tab.inactiveBackground": "#252526",
            "editorOverviewRuler.border": "#252526",
            "editor.inactiveSelectionBackground": "#689d6a20",
            "editor.selectionBackground": "#689d6a40",
            "editor.selectionHighlightBackground": "#fabd2f40",

            "editorLineNumber.foreground": "#858585",
            "editorLineNumber.activeForeground": "#858585"
        }
    },
    "vim.leader": ",",
    "vim.statusBarColorControl": false,
    "vim.hlsearch": true,
    "vim.cursorStylePerMode.visualblock": "block",
    "vim.cursorStylePerMode.insert": "block",
    "vim.cursorStylePerMode.normal": "block",
    "vim.cursorStylePerMode.replace": "block",
    "vim.cursorStylePerMode.visual": "block",
    "vim.cursorStylePerMode.visualline": "block",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["]", "g"],
            "commands": ["editor.action.marker.nextInFiles"],
            "silent": true
        },
        {
            "before": ["[", "g"],
            "commands": ["editor.action.marker.prevInFiles"],
            "silent": true
        },
        {
            "before": ["<leader>", "d", "f"],
            "commands": ["editor.action.formatDocument"],
            "silent": true
        },
        {
            "before": ["<leader>", "d", "s"],
            "commands": [":nohl"],
            "silent": true
        },
        {
            "before": ["<leader>", "t", "a"],
            "commands": ["workbench.action.toggleActivityBarVisibility"],
            "silent": true
        },
        {
            "before": ["<leader>", "t", "l"],
            "commands": ["workbench.view.extension.todo-tree-container"],
            "silent": true
        },
        {
            "before": ["<leader>", "d", "a"],
            "commands": ["workbench.debug.viewlet.action.removeAllBreakpoints"],
            "silent": true
        },
        {
            "before": ["<leader>", "d", "t"],
            "commands": [
                "workbench.action.debug.stop",
                // "workbench.action.toggleSidebarVisibility",
                "workbench.debug.action.toggleRepl",
                "workbench.debug.action.toggleRepl"
            ],
            "silent": true
        },
        {
            "before": ["<leader>", "t", "m"],
            "commands": ["editor.action.toggleMinimap"],
            "silent": true
        },
        {
            "before": ["%"],
            "commands": ["extension.matchitJumpItems"],
            "silent": true
        },
        {
            "before": ["<leader>", "s", "l"],
            "commands": ["workbench.action.splitEditorRight"],
            "silent": true
        },
        {
            "before": ["<leader>", "s", "j"],
            "commands": ["workbench.action.splitEditorDown"],
            "silent": true
        },
        {
            "before": ["<leader>", "r", "d"],
            "commands": ["editor.action.revealDeclaration"],
            "silent": true
        },
        {
            "before": ["<C-u>"],
            "after": ["1", "0", "k"]
        },
        {
            "before": ["<C-d>"],
            "after": ["1", "0", "j"]
        },
        {
            "before": ["<leader>", "c", "a"],
            "commands": ["workbench.action.closeEditorsInGroup"],
            "silent": true
        }
    ],
    "vim.commandLineModeKeyBindings": [
        {
            "before": ["W"],
            "after": ["w"]
        },
        {
            "before": ["Q"],
            "after": ["q"]
        }
    ],
    "csharpextensions.privateMemberPrefix": "_",
    "[csharp]": {
        "editor.defaultFormatter": "ms-dotnettools.csharp"
        // "editor.defaultFormatter": "csharpier.csharpier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "scm.diffDecorations": "none",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "terminal.integrated.cursorBlinking": true,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.renderFinalNewline": "off",
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "keyboard.dispatch": "keyCode",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "breadcrumbs.enabled": false,
    "editor.bracketPairColorization.enabled": false,
    "csharpextensions.useThisForCtorAssignments": false,
    "html.autoCreateQuotes": false,
    "csharpextensions.useFileScopedNamespace": true,
    "prettier.tabWidth": 4,
    "editor.inlineSuggest.enabled": false,
    "editor.wordWrap": "on",
    "vssolution.netcoreIgnore": ["node_modules", "obj", ".ds_store", ".vs"],
    "editor.insertSpaces": false,
    "editor.cursorSurroundingLines": 8,
    "terminal.integrated.fontSize": 17,
    "editor.fontSize": 17,
    "scm.diffDecorationsGutterPattern": {
        "added": true
    },
    "editor.lineNumbers": "relative",
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "window.customMenuBarAltFocus": false,
    "window.enableMenuBarMnemonics": false,
    "window.menuBarVisibility": "hidden",
    "settings.cycle": [
        {
            "id": "toggleMenuBar",
            "values": [
                { "window.menuBarVisibility": "hidden" },
                { "window.menuBarVisibility": "visible" }
            ]
        }
    ],
    "errorLens.scrollbarHackEnabled": true,
    "terminal.integrated.inheritEnv": true,
    // "gdscript_formatter.line_size": 80,
    // "godot_tools.gdscript_lsp_server_port": 6005,
    "errorLens.onSave": true,
    "omnisharp.enableEditorConfigSupport": false,
    "editor.occurrencesHighlight": false,
    "editor.minimap.enabled": false,
    "[markdown]": {
        "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
    },
    "workbench.activityBar.visible": false,
    "workbench.colorTheme": "Default Dark+ Experimental"
}
