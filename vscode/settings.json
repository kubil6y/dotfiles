{
    "editor.renderLineHighlight": "none",
    "editor.codeLens": false,
    "editor.lightbulb.enabled": "off",
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.hideCursorInOverviewRuler": true,
    "editor.scrollbar.vertical": "hidden",
    "editor.guides.indentation": false,
    "editor.renderWhitespace": "none",
    "editor.matchBrackets": "near",
    "editor.cursorStyle": "block",
    "explorer.compactFolders": false,
    "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
    "files.restoreUndoStack": false,
    "files.associations": {
        "*.html": "html"
    },
    "files.exclude": {
        ".godot/": true,
        "**/*.meta": true,
        "**/*.o": true,
        "**/obj": true
    },
    "search.exclude": {
        "**/*.anim": true
    },
    "go.toolsManagement.autoUpdate": true,
    // "omnisharp.enableImportCompletion": true,
    "omnisharp.useEditorFormattingSettings": false,
    // .net:true, unity:false
    "omnisharp.useModernNet": false,
    "omnisharp.monoPath": "/usr/bin/mono",
    "omnisharp.enableAsyncCompletion": true,
    "security.workspace.trust.enabled": false,
    "terminal.integrated.defaultProfile.linux": "zsh",
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "workbench.layoutControl.enabled": false,
    "workbench.sideBar.location": "left",
    "workbench.tree.indent": 12,
    "workbench.editor.restoreViewState": false,
    "workbench.startupEditor": "none",
    "editor.semanticHighlighting.enabled": true,
    "editor.semanticTokenColorCustomizations": {
        "[Vesper]": {
            "enabled": true,
            "rules": {
                "event": "#e0e0e0",
                "variable": "#e0e0e0",
                "property": "#e0e0e0",
                "parameter": "#e0e0e0",
                "type": "#e0e0e0",
                "enumMember": "#e0e0e0",
                "punctuation": "#e0e0e0",
            }
        },
    },
    "editor.tokenColorCustomizations": {
        "[Andromeda Zed]": {
            "textMateRules": [
                {
                    "scope": [
                        "constant.language.boolean",
                        "keyword.other.boolean",
                        "constant.language.null",
                        "keyword.other.null",
                        "constant.language"
                    ],
                    "settings": {
                        "foreground": "#FEE56B" // Replace with your preferred color
                    }
                },
                {
                    "scope": [
                        "entity.name.type",
                        "entity.name.type.class",
                        "entity.name.type.struct",
                        "entity.name.type.enum",
                        "entity.name.type.interface",
                        "keyword.type"
                    ],
                    "settings": {
                        "foreground": "#08e7c5ff" // Desired color
                    }
                },
                {
                    "scope": ["keyword.operator", "variable.language"],
                    "settings": {
                        "foreground": "#10A694" // Desired color
                    }
                },
                {
                    "scope": [
                        "comment",
                        "comment.block",
                        "comment.block.documentation",
                        "comment.line",
                        "constant",
                        "constant.character",
                        "constant.character.escape",
                        "constant.numeric",
                        "constant.numeric.integer",
                        "constant.numeric.float",
                        "constant.numeric.hex",
                        "constant.numeric.octal",
                        "constant.other",
                        "constant.regexp",
                        "constant.rgb-value",
                        "emphasis",
                        "entity",
                        "entity.name",
                        "entity.name.class",
                        "entity.name.function",
                        "entity.name.method",
                        "entity.name.section",
                        "entity.name.selector",
                        "entity.name.tag",
                        "entity.name.type",
                        "entity.other",
                        "entity.other.attribute-name",
                        "entity.other.inherited-class",
                        "invalid",
                        "invalid.deprecated",
                        "invalid.illegal",
                        "keyword",
                        "keyword.control",
                        "keyword.operator",
                        "keyword.operator.new",
                        "keyword.operator.assignment",
                        "keyword.operator.arithmetic",
                        "keyword.operator.logical",
                        "keyword.other",
                        "markup",
                        "markup.bold",
                        "markup.changed",
                        "markup.deleted",
                        "markup.heading",
                        "markup.inline.raw",
                        "markup.inserted",
                        "markup.italic",
                        "markup.list",
                        "markup.list.numbered",
                        "markup.list.unnumbered",
                        "markup.other",
                        "markup.quote",
                        "markup.raw",
                        "markup.underline",
                        "markup.underline.link",
                        "meta",
                        "meta.block",
                        "meta.cast",
                        "meta.class",
                        "meta.function",
                        "meta.function-call",
                        "meta.preprocessor",
                        "meta.return-type",
                        "meta.selector",
                        "meta.tag",
                        "meta.type.annotation",
                        "meta.type",
                        "punctuation.definition.string.begin",
                        "punctuation.definition.string.end",
                        "punctuation.separator",
                        "punctuation.separator.continuation",
                        "punctuation.terminator",
                        "storage",
                        "storage.modifier",
                        "storage.type",
                        "string",
                        "string.interpolated",
                        "string.other",
                        "string.quoted",
                        "string.quoted.double",
                        "string.quoted.other",
                        "string.quoted.single",
                        "string.quoted.triple",
                        "string.regexp",
                        "string.unquoted",
                        "strong",
                        "support",
                        "support.class",
                        "support.constant",
                        "support.function",
                        "support.other",
                        "support.type",
                        "support.type.property-name",
                        "support.variable",
                        "variable",
                        "variable.language",
                        "variable.name",
                        "variable.other",
                        "variable.other.readwrite",
                        "variable.parameter",
                        "comment.line.double-slash",
                        "punctuation.definition.comment"
                    ],
                    "settings": {
                        "fontStyle": ""
                    }
                },
                {
                    "scope": [
                        "comment",
                        "comment.block",
                        "comment.block.documentation",
                        "comment.line"
                    ],
                    "settings": {
                        "fontStyle": "",
                        "foreground": "#666d77"
                    }
                }
            ]
        },
        "[Vesper]": {
            "textMateRules": [
                {
                    "scope": [
                        "entity.name.variable",
                        "meta.paragraph.markdown",
                        "markup.list.unnumbered.markdown",
                        "text.html.markdown",
                        // "support.type.property-name.json.comments",
                    ],
                    "settings": {
                        "foreground": "#e0e0e0"
                    }
                }
            ]
        },
        "[Rosé Pine Moon (no italics)]": {
            "textMateRules": [
                {
                    "scope": ["entity.name.variable"],
                    "settings": {
                        "foreground": "#E0DEF4"
                    }
                }
                // {
                //     "scope": [
                //         "entity.name.type.class",
                //         "entity.name.section",
                //         "entity.name.tag",
                //         "entity.name.namespace",
                //         "entity.name.type",
                //     ],
                //     "settings": {
                //         // 6BB4C8,5BB3AE
                //         "foreground": "#5BB3AE"
                //     }
                // }
            ]
        }
    },
    "workbench.colorCustomizations": {
