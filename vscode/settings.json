            // tab theming (compact size)
            // "editorGroupHeader.tabsBackground": "#232033",
            "tab.activeBackground": "#191724",
            "tab.activeForeground": "#E0DEF4",
            "tab.inactiveForeground": "#6E6A86",
            "tab.activeBorderTop": "#6E6A86",

            // cursor
            "editorCursor.background": "#6e6a86",
            "editorCursor.foreground": "#e6edf3",
            "terminalCursor.background": "#6e6a86",
            "terminalCursor.foreground": "#e6edf3",

            // autocomplete stuff
            "list.highlightForeground": "#e0def4",
            "list.focusHighlightForeground": "#e0def4",
            "editorSuggestWidget.focusHighlightForeground": "#e0def4",
            "editorSuggestWidget.highlightForeground": "#e0def4",

            "editorLineNumber.activeForeground": "#5c5877",
            "editorLineNumber.foreground": "#5c5877",

            "editor.selectionBackground": "#3b3551"
        },
        "[Default Dark Modern]": {
            // "editorStickyScroll.background": "#303030",
            "editorLineNumber.activeForeground": "#4a4f56",
            "editorLineNumber.foreground": "#4a4f56"
        }
    },
    "vim.leader": " ",
    "vim.statusBarColorControl": false,
    "vim.hlsearch": true,
    "vim.cursorStylePerMode.visualblock": "block",
    "vim.cursorStylePerMode.insert": "block",
    "vim.cursorStylePerMode.normal": "block",
    "vim.cursorStylePerMode.replace": "block",
    "vim.cursorStylePerMode.visual": "block",
    "vim.cursorStylePerMode.visualline": "block",
    "vim.handleKeys": {
        "<Esc>": false
    },
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["]", "d"],
            "commands": ["editor.action.marker.nextInFiles"],
            "silent": true
        },
        {
            "before": ["[", "d"],
            "commands": ["editor.action.marker.prevInFiles"],
            "silent": true
        },
        {
            "before": ["<leader>", "f"],
            "commands": ["editor.action.formatDocument"],
            "silent": true
        },
        {
            "before": ["<leader>", "d", "s"],
            "commands": [":nohl"],
            "silent": true
        },
        {
            "before": ["<leader>", "d", "a"],
            "commands": ["workbench.debug.viewlet.action.removeAllBreakpoints"],
            "silent": true
        },
        {
            "before": ["<leader>", "t", "a"],
            "commands": ["workbench.action.toggleActivityBarVisibility"],
            "silent": true
        },
        {
            "before": ["<leader>", "s", "l"],
            "commands": ["workbench.view.extension.sln_explorer"],
            "silent": true
        },
        {
            "before": ["<leader>", "t", "l"],
            "commands": ["workbench.view.extension.todo-tree-container"],
            "silent": true
        },
        {
            "before": ["<leader>", "t", "m"],
            "commands": ["editor.action.toggleMinimap"],
            "silent": true
        },
        {
            "before": ["%"],
            "commands": ["extension.matchitJumpItems"],
            "silent": true
        },
        {
            "before": ["<leader>", "d", "t"],
            "commands": ["editor.action.revealDeclaration"],
            "silent": true
        },
        {
            "before": ["<C-d>"],
            "after": ["<C-d>", "z", "z"]
        },
        {
            "before": ["<C-u>"],
            "after": ["<C-u>", "z", "z"]
        },
        {
            "before": ["<leader>", "c", "a"],
            "commands": ["workbench.action.closeEditorsInGroup"],
            "silent": true
        },
        {
            "before": ["<leader>", "c", "e"],
            "commands": ["workbench.action.closeOtherEditors"],
            "silent": true
        },
        {
            "before": ["<C-p>"],
            "commands": ["workbench.action.quickOpen"],
            "silent": true
        },
        {
            "before": ["<leader>", "d", "n"],
            "commands": ["notifications.clearAll"],
            "silent": true
        }
    ],
    "vim.commandLineModeKeyBindings": [
        {
            "before": ["W"],
            "after": ["w"]
        },
        {
            "before": ["Q"],
            "after": ["q"]
        }
    ],
    // "[csharp]": {
    //     "editor.defaultFormatter": "ms-dotnettools.csharp"
    //     // "editor.defaultFormatter": "csharpier.csharpier-vscode"
    // },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "scm.diffDecorations": "none",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.renderFinalNewline": "off",
    "keyboard.dispatch": "keyCode",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "breadcrumbs.enabled": false,
    "editor.bracketPairColorization.enabled": false,
    "csharpextensions.useThisForCtorAssignments": false,
    "html.autoCreateQuotes": false,
    "csharpextensions.useFileScopedNamespace": true,
    "editor.inlineSuggest.enabled": false,
    "vssolution.netcoreIgnore": ["node_modules", "obj", ".ds_store", ".vs"],
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "prettier.tabWidth": 4,
    "prettier.useTabs": false,
    "scm.diffDecorationsGutterPattern": {
        "added": true
    },
    "editor.lineNumbers": "relative",
    "window.customMenuBarAltFocus": false,
    "window.enableMenuBarMnemonics": false,
    "window.menuBarVisibility": "hidden",
    "settings.cycle": [
        {
            "id": "toggleMenuBar",
            "values": [
                { "window.menuBarVisibility": "hidden" },
                { "window.menuBarVisibility": "visible" }
            ]
        }
    ],
    "errorLens.scrollbarHackEnabled": true,
    "terminal.integrated.inheritEnv": true,
    // "gdscript_formatter.line_size": 80,
    // "godot_tools.gdscript_lsp_server_port": 6005,
    "errorLens.onSave": true,
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.iconTheme": "material-icon-theme",
    "editor.occurrencesHighlight": "off",
    "extensions.ignoreRecommendations": true,
    "editor.minimap.enabled": false,
    "window.zoomLevel": 1,
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "editor.scrollbar.verticalScrollbarSize": 0,
    "editor.stickyScroll.enabled": false,
    "editor.stickyScroll.maxLineCount": 1,
    "todo-tree.general.tags": ["BUG", "HACK", "FIXME", "TODO", "XXX"],
    // "editor.fontFamily": "Berkeley Mono",
    "editor.fontFamily": "MonoLisa",
    "editor.fontLigatures": "'liga' on, 'zero', 'ss01' on,'ss02' on, 'ss04' on, 'ss05' on, 'ss07' on, 'ss13' on, 'ss14' on, 'ss16' on, 'ss18' on, 'ss15' on, 'ss17' on",
    "editor.fontSize": 15,
    "editor.suggestFontSize": 15,
    "terminal.integrated.fontSize": 15,
    "editor.fontVariations": true,
    "editor.wordWrap": "on",
    "editor.suggestSelection": "first",
    "dotnet.formatting.organizeImportsOnFormat": true,
    // tab sizing
    "window.density.editorTabHeight": "compact",
    "workbench.editor.showIcons": false,
    "workbench.editor.enablePreview": false,
    "window.titleBarStyle": "custom",
    "chat.commandCenter.enabled": false,
    "window.commandCenter": false,
    "editor.cursorBlinking": "solid",
    "workbench.activityBar.location": "hidden",
    "workbench.colorTheme": "Vesper"
    // tab sizing
}
