{
    "editor.fontFamily": "MonoLisa",
    "editor.suggestSelection": "first",
    "editor.renderLineHighlight": "none",
    "editor.codeLens": false,
    "editor.lightbulb.enabled": false,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.hideCursorInOverviewRuler": true,
    "editor.scrollbar.vertical": "hidden",
    "editor.guides.indentation": false,
    "editor.renderWhitespace": "none",
    "editor.matchBrackets": "near",
    "explorer.compactFolders": false,
    "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
    "files.restoreUndoStack": false,
    "files.associations": {
        "*.html": "html"
    },
    "files.exclude": {
        ".godot/": true,
        "**/*.meta": true,
        "**/*.o": true,
        "**/obj": true
    },
    "go.toolsManagement.autoUpdate": true,
    // "omnisharp.enableImportCompletion": true,
    "omnisharp.useEditorFormattingSettings": false,
    "omnisharp.organizeImportsOnFormat": true,
    // .net:true, unity:false
    "omnisharp.useModernNet": false,
    "omnisharp.monoPath": "/usr/bin/mono",
    "omnisharp.enableAsyncCompletion": true,
    "security.workspace.trust.enabled": false,
    "terminal.integrated.defaultProfile.linux": "bash",
    "terminal.integrated.persistentSessionReviveProcess": "never",
    // "workbench.editor.enablePreview": false,
    "workbench.layoutControl.enabled": false,
    "workbench.sideBar.location": "left",
    "workbench.tree.indent": 12,
    "workbench.editor.restoreViewState": false,
    "workbench.startupEditor": "none",
    "editor.tokenColorCustomizations": {
        "[Monokai Pro (Filter Ristretto)]": {
            "textMateRules": [
                {
                    "scope": [
                        "storage.type",
                        "variable.parameter",
                        "parameters variable.function",
                        "support.type",
                        "storage.modifier",
                        "entity.other.inherited-class",
                        "entity.other.attribute-name",
                        "comment",
                        "punctuation.definition.comment",
                        "invalid"
                    ],
                    "settings": {
                        "fontStyle": ""
                    }
                }
            ]
        },
        "[Dark Rock]": {
            "variables": "#ebdbb2",
            "textMateRules": [
                {
                    "scope": [
                        "entity.name.class.static.function",
                        "entity.name.function"
                    ],
                    "settings": {
                        "fontStyle": "bold",
                        "foreground": "#fabd2f"
                    }
                },
                {
                    "scope": [
                        "keyword.type",
                        "keyword.control",
                        "keyword.operator.expression",
                        "keyword.other",
                        "storage.modifier",
                        "storage.type.class",
                        "storage.type.accessor",
                        "storage.type.enum"
                    ],
                    "settings": {
                        "foreground": "#FB4934"
                    }
                },
                {
                    "scope": [
                        "entity.name.type",
                        "entity.name.type.class",
                        "storage.type"
                    ],
                    "settings": {
                        "foreground": "#8EC07C"
                    }
                }
            ]
        },
        "[Default Dark Modern]": {
            "variables": "#d4d4d4",
            "textMateRules": [
                {
                    "scope": [
                        "variable.other.constant.object.ts",
                        "variable.other.constant.object.js",
                        "variable.other.constant.ts",
                        "meta.definition.variable.ts",
                        "meta.var-single-variable.expr.ts",
                        "meta.var.expr.ts",
                        "source.ts",
                        "support.type.property-name",
                        "variable.other.enummember"
                    ],
                    "settings": {
                        "foreground": "#d4d4d4"
                    }
                },
                {
                    "scope": "emphasis",
                    "settings": {
                        "fontStyle": ""
                    }
                },
                {
                    "scope": "keyword.preprocessor.cs",
                    "settings": {
                        "foreground": "#808080"
                    }
                }
            ]
        }
    },
    "workbench.colorCustomizations": {
        "[Quiet Light]": {
            "statusBar.foreground": "#705697",
            "statusBar.background": "#f5f5f5",
            "statusBar.noFolderBackground": "#f5f5f5",

            "editorCursor.foreground": "#121212",
            "editorCursor.background": "#f5f5f5",

            "editorLineNumber.activeForeground": "#AAAAAA",
            "editorLineNumber.foreground": "#AAAAAA"
        },
        "[Dark Rock]": {
            "editor.background": "#1d2021",
            "statusBar.background": "#1d2021",
            "statusBar.border": "#1d2021",

            "editorLineNumber.activeForeground": "#504945",
            "editorLineNumber.foreground": "#504945",

            "list.highlightForeground": "#ebdbb2"
        },

        "[Default Dark Modern]": {
            "statusBar.background": "#1f1f1f",
            "statusBar.border": "#1f1f1f",
            "editorLineNumber.activeForeground": "#6e7681"
        },
        "[Monokai Pro (Filter Ristretto)]": {
            "statusBar.background": "#2c2525",
            "statusBar.border": "#2c2525",
            "editorGroup.emptyBackground": "#2c2525",
            "activityBar.background": "#2c2525",

            "tab.activeBorderTop": "#f9cc6c",
            "tab.activeBorder": "#2c2525",
            "tab.unfocusedActiveBorder": "#2c2525",
            "tab.unfocusedActiveBorderTop": "#948a8b",

            "editorHoverWidget.highlightForeground": "#ffd866",
            "editorLineNumber.activeForeground": "#5b5353"
        },
        "[Ros√© Pine (no italics)]": {
            "list.highlightForeground": "#e0def4",
            "list.focusHighlightForeground": "#e0def4",
            "editorSuggestWidget.focusHighlightForeground": "#e0def4",
            "editorSuggestWidget.highlightForeground": "#908caa",

            "editorCursor.background": "#191724",
            "editorCursor.foreground": "#e0def4",
            "terminalCursor.background": "#191724",
            "terminalCursor.foreground": "#e0def4",

            "tab.activeBorderTop": "#31748f",
            "editorLineNumber.activeForeground": "#908caa",
            "terminal.ansiBrightBlue": "#31748f"
        }
    },
    "vim.leader": ",",
    "vim.statusBarColorControl": false,
    "vim.hlsearch": true,
    "vim.cursorStylePerMode.visualblock": "block",
    "vim.cursorStylePerMode.insert": "block",
    "vim.cursorStylePerMode.normal": "block",
    "vim.cursorStylePerMode.replace": "block",
    "vim.cursorStylePerMode.visual": "block",
    "vim.cursorStylePerMode.visualline": "block",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["]", "g"],
            "commands": ["editor.action.marker.nextInFiles"],
            "silent": true
        },
        {
            "before": ["[", "g"],
            "commands": ["editor.action.marker.prevInFiles"],
            "silent": true
        },
        {
            "before": ["<leader>", "d", "f"],
            "commands": ["editor.action.formatDocument"],
            "silent": true
        },
        {
            "before": ["<leader>", "d", "s"],
            "commands": [":nohl"],
            "silent": true
        },
        {
            "before": ["<leader>", "d", "a"],
            "commands": ["workbench.debug.viewlet.action.removeAllBreakpoints"],
            "silent": true
        },
        {
            "before": ["<leader>", "t", "a"],
            "commands": ["workbench.action.toggleActivityBarVisibility"],
            "silent": true
        },
        {
            "before": ["<leader>", "t", "l"],
            "commands": ["workbench.view.extension.todo-tree-container"],
            "silent": true
        },
        {
            "before": ["<leader>", "t", "m"],
            "commands": ["editor.action.toggleMinimap"],
            "silent": true
        },
        {
            "before": ["%"],
            "commands": ["extension.matchitJumpItems"],
            "silent": true
        },
        {
            "before": ["<leader>", "d", "t"],
            "commands": ["editor.action.revealDeclaration"],
            "silent": true
        },
        {
            "before": ["<C-u>"],
            "after": ["1", "0", "k"]
        },
        {
            "before": ["<C-d>"],
            "after": ["1", "0", "j"]
        },
        {
            "before": ["<leader>", "c", "a"],
            "commands": ["workbench.action.closeEditorsInGroup"],
            "silent": true
        },
        {
            "before": ["<leader>", "c", "e"],
            "commands": ["workbench.action.closeOtherEditors"],
            "silent": true
        },
        {
            "before": ["<C-p>"],
            "commands": ["workbench.action.quickOpen"],
            "silent": true
        },
        {
            "before": ["<leader>", "d", "n"],
            "commands": ["notifications.clearAll"],
            "silent": true
        }
    ],
    "vim.commandLineModeKeyBindings": [
        {
            "before": ["W"],
            "after": ["w"]
        },
        {
            "before": ["Q"],
            "after": ["q"]
        }
    ],
    "csharpextensions.privateMemberPrefix": "_",
    // "[csharp]": {
    //     "editor.defaultFormatter": "ms-dotnettools.csharp"
    //     // "editor.defaultFormatter": "csharpier.csharpier-vscode"
    // },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "scm.diffDecorations": "none",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "terminal.integrated.cursorBlinking": true,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.renderFinalNewline": "off",
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "keyboard.dispatch": "keyCode",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "breadcrumbs.enabled": false,
    "editor.bracketPairColorization.enabled": false,
    "csharpextensions.useThisForCtorAssignments": false,
    "html.autoCreateQuotes": false,
    "csharpextensions.useFileScopedNamespace": true,
    "prettier.tabWidth": 4,
    "editor.inlineSuggest.enabled": false,
    "editor.wordWrap": "on",
    "vssolution.netcoreIgnore": ["node_modules", "obj", ".ds_store", ".vs"],
    "editor.insertSpaces": false,
    "editor.cursorSurroundingLines": 8,
    "terminal.integrated.fontSize": 14,
    "editor.fontSize": 14,
    "scm.diffDecorationsGutterPattern": {
        "added": true
    },
    "editor.lineNumbers": "relative",
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "window.customMenuBarAltFocus": false,
    "window.enableMenuBarMnemonics": false,
    "window.menuBarVisibility": "hidden",
    "settings.cycle": [
        {
            "id": "toggleMenuBar",
            "values": [
                { "window.menuBarVisibility": "hidden" },
                { "window.menuBarVisibility": "visible" }
            ]
        }
    ],
    "errorLens.scrollbarHackEnabled": true,
    "terminal.integrated.inheritEnv": true,
    // "gdscript_formatter.line_size": 80,
    // "godot_tools.gdscript_lsp_server_port": 6005,
    "errorLens.onSave": true,
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.minimap.enabled": false,
    "window.zoomLevel": 1,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.activityBar.visible": false,
    "editor.occurrencesHighlight": false,
    "workbench.colorTheme": "Dark Rock"
}
