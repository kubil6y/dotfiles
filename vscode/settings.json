{
    "editor.fontFamily": "MonoLisa",
    "editor.suggestSelection": "first",
    "editor.renderLineHighlight": "none",
    "editor.lightbulb.enabled": false,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.hideCursorInOverviewRuler": true,
    "editor.scrollbar.vertical": "hidden",
    "editor.codeLens": false,
    "editor.guides.indentation": false,
    "editor.renderWhitespace": "none",
    "editor.matchBrackets": "near",
    "explorer.compactFolders": false,
    "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
    "files.restoreUndoStack": false,
    "files.associations": {
        "*.html": "html"
    },
    "files.exclude": {
        "**/obj": true,
        "**/*.meta": true
    },
    "go.toolsManagement.autoUpdate": true,
    // "omnisharp.enableImportCompletion": true,
    // "omnisharp.useEditorFormattingSettings": true,
    "omnisharp.organizeImportsOnFormat": true,
    // .net:true, unity:false
    "omnisharp.useModernNet": false,
    "omnisharp.monoPath": "/usr/bin/mono",
    "omnisharp.enableAsyncCompletion": true,
    "security.workspace.trust.enabled": false,
    "terminal.integrated.defaultProfile.linux": "bash",
    "terminal.integrated.persistentSessionReviveProcess": "never",
    // "workbench.editor.enablePreview": false,
    "workbench.layoutControl.enabled": false,
    "workbench.sideBar.location": "left",
    "workbench.tree.indent": 12,
    "workbench.editor.restoreViewState": false,
    "workbench.startupEditor": "none",
    "workbench.iconTheme": "material-icon-theme",
    "editor.tokenColorCustomizations": {
        "[Default Dark+]": {
            // D4D4D4,FFF1F1F1,e2e2e2
            "variables": "#e2e2e2",
            "textMateRules": [
                {
                    "scope": [
                        "variable.other.constant.object.ts",
                        "variable.other.constant.object.js"
                    ],
                    "settings": {
                        "foreground": "#e2e2e2"
                    }
                },
                {
                    "scope": "keyword.preprocessor.cs",
                    "settings": {
                        "foreground": "#808080"
                    }
                },
                {
                    "scope": "support.type.property-name",
                    "settings": {
                        "foreground": "#e2e2e2"
                    }
                },
                {
                    "scope": "variable.other.enummember",
                    "settings": {
                        "foreground": "#e2e2e2"
                    }
                }
            ]
        },
        "[Monokai Pro (Filter Ristretto)]": {
            "textMateRules": [
                {
                    "scope": [
                        "storage.type",
                        "variable.parameter",
                        "parameters variable.function",
                        "support.type",
                        "storage.modifier",
                        "entity.other.inherited-class",
                        "entity.other.attribute-name",
                        "comment",
                        "punctuation.definition.comment",
                        "invalid"
                    ],
                    "settings": {
                        "fontStyle": ""
                    }
                },
                {
                    "scope": [
                        "comment.git-status.header.local",
                        "constant.other.elm",
                        "entity.name.type.class.templated",
                        "entity.name.type.class.generic",
                        "entity.name.type.namespace",
                        "entity.name.function.preprocessor",
                        "entity.name.class",
                        "entity.other.inherited-class",
                        "entity.name.type",
                        "entity.name.type.class.reference",
                        "entity.name.type.class.value",
                        "entity.other.attribute-name",
                        "entity.other.attribute-name.pseudo-class.css",
                        "entity.other.pseudo-class.css",
                        "entity.other.pseudo-element.css",
                        "entity.name.type.rust",
                        "keyword.other.fn.rust",
                        "keyword.other.rust",
                        "keyword.other.unsafe.rust",
                        "keyword.other.where.rust",
                        "storage.type",
                        "storage.modifier.const.rust",
                        "storage.modifier.dyn.rust",
                        "storage.modifier.mut.rust",
                        "storage.modifier.static.rust",
                        "storage.type.rust",
                        "storage.type.core.rust",
                        "storage.class.std.rust",
                        "string.other.link.description.title",
                        "support.constant",
                        "support.type",
                        "entity.name.type.object.console",
                        "support.variable",
                        "support.class",
                        "variable.other.class"
                    ],
                    "settings": {
                        "foreground": "#4EC9B0",
                        "fontStyle": ""
                    }
                }
            ]
        },
        "[Gruvbox Dark (Hard)]": {
            "textMateRules": [
                {
                    "scope": [
                        "entity.name.class.static.function",
                        "entity.name.function"
                    ],
                    "settings": {
                        "fontStyle": "bold"
                    }
                },
                {
                    "scope": ["comment", "punctuation.definition.comment"],
                    "settings": {
                        "fontStyle": ""
                    }
                }
            ]
        }
    },
    "workbench.colorCustomizations": {
        "[Default Dark+]": {
            "activityBar.background": "#1e1e1e",
            "statusBar.background": "#1e1e1e",
            "tab.activeBorder": "#D4D4D4", // 4EC9B0,
            "tab.inactiveBackground": "#252526",
            "editorOverviewRuler.border": "#1e1e1e",
            "editorLineNumber.foreground": "#858585",
            "editorLineNumber.activeForeground": "#858585"
        },
        "[Monokai Pro (Filter Ristretto)]": {
            // 2d2a2e for monokai pro
            "statusBar.background": "#2c2525",
            "editorGroup.emptyBackground": "#2c2525",
            "tab.activeBorder": "#ffd866",
            "editorHoverWidget.highlightForeground": "#ffd866",
            "activityBar.background": "#2c2525",
            "editorLineNumber.activeForeground": "#5b5353"
        },
        "[Gruvbox Dark (Hard)]": {
            // "editorError.foreground": "#d79921",
            "editorInlayHint.foreground": "#868686f0",
            "editorInlayHint.background": "#18181800",
            // Overrides for specific kinds of inlay hints
            "editorInlayHint.typeForeground": "#486148f0",
            "editorInlayHint.parameterForeground": "#486148f0",

            "activityBar.background": "#1d2021",
            "statusBar.background": "#1d2021", //282828,1d2021
            "statusBar.foreground": "#858585",
            "tab.activeBorder": "#d79921",
            "tab.inactiveBackground": "#252526",
            "editorOverviewRuler.border": "#252526",
            "editor.inactiveSelectionBackground": "#689d6a20",
            "editor.selectionBackground": "#689d6a40",
            "editor.selectionHighlightBackground": "#fabd2f40",

            "editorLineNumber.foreground": "#858585",
            "editorLineNumber.activeForeground": "#858585"
            // sticky scroll settings
            // "editorStickyScroll.background": "#444",
            // "editorStickyScrollHover.background": "#f00",
            // "scrollbar.shadow": "#fff9", // with a little opacity for the dropShadow
        }
    },
    "vim.leader": ",",
    "vim.statusBarColorControl": false,
    "vim.hlsearch": true,
    "vim.cursorStylePerMode.visualblock": "block",
    "vim.cursorStylePerMode.insert": "block",
    "vim.cursorStylePerMode.normal": "block",
    "vim.cursorStylePerMode.replace": "block",
    "vim.cursorStylePerMode.visual": "block",
    "vim.cursorStylePerMode.visualline": "block",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["]", "g"],
            "commands": ["editor.action.marker.nextInFiles"],
            "silent": true
        },
        {
            "before": ["[", "g"],
            "commands": ["editor.action.marker.prevInFiles"],
            "silent": true
        },
        {
            "before": ["<leader>", "d", "f"],
            "commands": ["editor.action.formatDocument"],
            "silent": true
        },
        {
            "before": ["<leader>", "d", "s"],
            "commands": [":nohl"],
            "silent": true
        },
        {
            "before": ["<leader>", "t", "a"],
            "commands": ["workbench.action.toggleActivityBarVisibility"],
            "silent": true
        },
        {
            "before": ["<leader>", "t", "l"],
            "commands": ["workbench.view.extension.todo-tree-container"],
            "silent": true
        },
        {
            "before": ["<leader>", "d", "a"],
            "commands": ["workbench.debug.viewlet.action.removeAllBreakpoints"],
            "silent": true
        },
        {
            "before": ["<leader>", "d", "t"],
            "commands": [
                "workbench.action.debug.stop",
                // "workbench.action.toggleSidebarVisibility",
                "workbench.debug.action.toggleRepl",
                "workbench.debug.action.toggleRepl"
            ],
            "silent": true
        },
        {
            "before": ["<leader>", "t", "m"],
            "commands": ["editor.action.toggleMinimap"],
            "silent": true
        },
        {
            "before": ["%"],
            "commands": ["extension.matchitJumpItems"],
            "silent": true
        },
        {
            "before": ["<leader>", "s", "l"],
            "commands": ["workbench.action.splitEditorRight"],
            "silent": true
        },
        {
            "before": ["<leader>", "s", "j"],
            "commands": ["workbench.action.splitEditorDown"],
            "silent": true
        },
        {
            "before": ["g", "f"],
            "commands": ["editor.action.revealDeclaration"],
            "silent": true
        },
        {
            "before": ["<leader>", "m", "l"],
            "commands": ["workbench.action.moveActiveEditorGroupRight"],
            "silent": true
        },
        {
            "before": ["<leader>", "m", "h"],
            "commands": ["workbench.action.moveActiveEditorGroupLeft"],
            "silent": true
        },
        {
            "before": ["<leader>", "m", "j"],
            "commands": ["workbench.action.moveActiveEditorGroupDown"],
            "silent": true
        },
        {
            "before": ["<leader>", "m", "k"],
            "commands": ["workbench.action.moveActiveEditorGroupUp"],
            "silent": true
        },
        {
            "before": ["<C-u>"],
            "after": ["1", "0", "k"]
        },
        {
            "before": ["<C-d>"],
            "after": ["1", "0", "j"]
        }
    ],
    "vim.commandLineModeKeyBindings": [
        {
            "before": ["W"],
            "after": ["w"]
        },
        {
            "before": ["E"],
            "after": ["e"]
        },
        {
            "before": ["Q"],
            "after": ["q"]
        }
    ],
    "csharpextensions.privateMemberPrefix": "_",
    "[csharp]": {
        "editor.defaultFormatter": "ms-dotnettools.csharp"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "scm.diffDecorations": "none",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "terminal.integrated.cursorBlinking": true,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.renderFinalNewline": false,
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "keyboard.dispatch": "keyCode",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "breadcrumbs.enabled": false,
    "editor.bracketPairColorization.enabled": false,
    "csharpextensions.useThisForCtorAssignments": false,
    "html.autoCreateQuotes": false,
    "csharpextensions.useFileScopedNamespace": true,
    "prettier.tabWidth": 4,
    "editor.inlineSuggest.enabled": false,
    "editor.wordWrap": "on",
    "vssolution.netcoreIgnore": ["node_modules", "obj", ".ds_store", ".vs"],
    "editor.insertSpaces": false,
    "editor.cursorSurroundingLines": 8,
    "editor.occurrencesHighlight": false,
    "terminal.integrated.fontSize": 17,
    "editor.fontSize": 17,
    "scm.diffDecorationsGutterPattern": {
        "added": true
    },
    "editor.lineNumbers": "relative",
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "window.customMenuBarAltFocus": false,
    "window.enableMenuBarMnemonics": false,
    "window.menuBarVisibility": "hidden",
    "settings.cycle": [
        {
            "id": "toggleMenuBar",
            "values": [
                { "window.menuBarVisibility": "hidden" },
                { "window.menuBarVisibility": "visible" }
            ]
        }
    ],
    "editor.minimap.enabled": false,
    "workbench.activityBar.visible": false,
    "liveServer.settings.donotShowInfoMsg": true
}
