{
    "editor.renderLineHighlight": "none",
    "editor.codeLens": false,
    "editor.lightbulb.enabled": "off",
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.hideCursorInOverviewRuler": true,
    "editor.scrollbar.vertical": "hidden",
    "editor.guides.indentation": false,
    "editor.renderWhitespace": "none",
    "editor.matchBrackets": "near",
    "editor.cursorStyle": "block",
    "explorer.compactFolders": false,
    "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
    "files.restoreUndoStack": false,
    "files.associations": {
        "*.html": "html"
    },
    "files.exclude": {
        ".godot/": true,
        "**/*.meta": true,
        "**/*.o": true,
        "**/obj": true
    },
    "search.exclude": {
        "**/*.anim": true
    },
    "go.toolsManagement.autoUpdate": true,
    // "omnisharp.enableImportCompletion": true,
    "omnisharp.useEditorFormattingSettings": false,
    "omnisharp.organizeImportsOnFormat": true,
    // .net:true, unity:false
    "omnisharp.useModernNet": false,
    "omnisharp.monoPath": "/usr/bin/mono",
    "omnisharp.enableAsyncCompletion": true,
    "security.workspace.trust.enabled": false,
    "terminal.integrated.defaultProfile.linux": "zsh",
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "workbench.layoutControl.enabled": false,
    "workbench.sideBar.location": "left",
    "workbench.tree.indent": 12,
    "workbench.editor.restoreViewState": false,
    "workbench.startupEditor": "none",
    "editor.tokenColorCustomizations": {
        "[Monokai Pro (Filter Octagon)]": {
            "textMateRules": [
                {
                    "scope": [
                        "comment",
                        "comment.block",
                        "comment.block.documentation",
                        "comment.line",
                        "constant",
                        "constant.character",
                        "constant.character.escape",
                        "constant.numeric",
                        "constant.numeric.integer",
                        "constant.numeric.float",
                        "constant.numeric.hex",
                        "constant.numeric.octal",
                        "constant.other",
                        "constant.regexp",
                        "constant.rgb-value",
                        "emphasis",
                        "entity",
                        "entity.name",
                        "entity.name.class",
                        "entity.name.function",
                        "entity.name.method",
                        "entity.name.section",
                        "entity.name.selector",
                        "entity.name.tag",
                        "entity.name.type",
                        "entity.other",
                        "entity.other.attribute-name",
                        "entity.other.inherited-class",
                        "invalid",
                        "invalid.deprecated",
                        "invalid.illegal",
                        "keyword",
                        "keyword.control",
                        "keyword.operator",
                        "keyword.operator.new",
                        "keyword.operator.assignment",
                        "keyword.operator.arithmetic",
                        "keyword.operator.logical",
                        "keyword.other",
                        "markup",
                        "markup.bold",
                        "markup.changed",
                        "markup.deleted",
                        "markup.heading",
                        "markup.inline.raw",
                        "markup.inserted",
                        "markup.italic",
                        "markup.list",
                        "markup.list.numbered",
                        "markup.list.unnumbered",
                        "markup.other",
                        "markup.quote",
                        "markup.raw",
                        "markup.underline",
                        "markup.underline.link",
                        "meta",
                        "meta.block",
                        "meta.cast",
                        "meta.class",
                        "meta.function",
                        "meta.function-call",
                        "meta.preprocessor",
                        "meta.return-type",
                        "meta.selector",
                        "meta.tag",
                        "meta.type.annotation",
                        "meta.type",
                        "punctuation.definition.string.begin",
                        "punctuation.definition.string.end",
                        "punctuation.separator",
                        "punctuation.separator.continuation",
                        "punctuation.terminator",
                        "storage",
                        "storage.modifier",
                        "storage.type",
                        "string",
                        "string.interpolated",
                        "string.other",
                        "string.quoted",
                        "string.quoted.double",
                        "string.quoted.other",
                        "string.quoted.single",
                        "string.quoted.triple",
                        "string.regexp",
                        "string.unquoted",
                        "strong",
                        "support",
                        "support.class",
                        "support.constant",
                        "support.function",
                        "support.other",
                        "support.type",
                        "support.type.property-name",
                        "support.variable",
                        "variable",
                        "variable.language",
                        "variable.name",
                        "variable.other",
                        "variable.other.readwrite",
                        "variable.parameter"
                    ],
                    "settings": {
                        "fontStyle": ""
                    }
                }
            ]
        },
        "[Rosé Pine (no italics)]": {
            "textMateRules": [
                {
                    "scope": ["entity.name.variable"],
                    "settings": {
                        "foreground": "#E0DEF4"
                    }
                },
            ]
        },
        "[Ayu Dark Bordered]": {
            "textMateRules": [
                {
                    "scope": [
                        "entity.name.function",
                        "support.function",
                        "meta.function-call",
                        "variable.function",
                        "meta.method",
                        "entity.name.method"
                    ],
                    "settings": {
                        // f07174,ffee99
                        "foreground": "#f07174"
                    }
                },
                {
                    "scope": "comment",
                    "settings": {
                        "fontStyle": ""
                    }
                },
                {
                    "scope": [
                        "variable.parameter",
                        "meta.parameter",
                        "variable.other.member",
                        "variable.other.property",
                        "variable.other.member",
                        "variable.other.property",
                        "entity.name.variable.field.cs",
                        "variable.other.readwrite.js",
                        "variable.other.readwrite.ts",
                        "variable.other.readwrite.go",
                        "entity.name.variable.field.go"
                    ],
                    "settings": {
                        "foreground": "#c9c7be"
                    }
                }
            ]
        }
    },
    "workbench.colorCustomizations": {
        "[Monokai Pro (Filter Octagon)]": {
            "tab.activeBorder": "#282a3a",
            "tab.activeBorderTop": "#ffd76d",
            "editorLineNumber.activeForeground": "#535763",
            "editorCursor.background": "#6e6a86",
            "editorCursor.foreground": "#e6edf3",
        },
        "[Rosé Pine (no italics)]": {
            // 13121c,191724
            "editor.background": "#191724",
            "sideBar.background": "#191724",
            "editorGutter.background": "#191724",
            "statusBar.background": "#191724",
            "welcomePage.background": "#191724",

            "editorStickyScroll.background": "#26233a",
            "editor.selectionBackground": "#44415a",
            "editorCursor.background": "#6e6a86",
            "editorCursor.foreground": "#e6edf3",
            "terminalCursor.background": "#6e6a86",
            "terminalCursor.foreground": "#e6edf3",
            "list.highlightForeground": "#e0def4",
            "list.focusHighlightForeground": "#e0def4",
            "editorSuggestWidget.focusHighlightForeground": "#e0def4",
            "editorSuggestWidget.highlightForeground": "#e0def4",
            "tab.activeBorderTop": "#31748f",
            "editorLineNumber.activeForeground": "#5c5877",
            "editorLineNumber.foreground": "#5c5877"
        },
        "[Ayu Dark Bordered]": {
            // "editor.background": "#1f2430",
            "editorCursor.background": "#6e6a86",
            "editorCursor.foreground": "#e6edf3",
            "editorSuggestWidget.selectedBackground": "#47526699",
            "list.activeSelectionBackground": "#47526699",
            "editorLineNumber.activeForeground": "#6c738099"
        },
        "[Default Dark Modern]": {
            // "editor.background": "#181818",
            // "editorOverviewRuler.border": "#181818",
            // "statusBar.border": "#181818",
            // "statusBar.background": "#181818",
            "editorStickyScroll.background": "#303030",
            // 6e7681,#4a4f56
            "editorLineNumber.activeForeground": "#4a4f56",
            "editorLineNumber.foreground": "#4a4f56"
        }
    },
    "vim.leader": " ",
    "vim.statusBarColorControl": false,
    "vim.hlsearch": true,
    "vim.cursorStylePerMode.visualblock": "block",
    "vim.cursorStylePerMode.insert": "block",
    "vim.cursorStylePerMode.normal": "block",
    "vim.cursorStylePerMode.replace": "block",
    "vim.cursorStylePerMode.visual": "block",
    "vim.cursorStylePerMode.visualline": "block",
    "vim.handleKeys": {
        "<Esc>": false
    },
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["]", "d"],
            "commands": ["editor.action.marker.nextInFiles"],
            "silent": true
        },
        {
            "before": ["[", "d"],
            "commands": ["editor.action.marker.prevInFiles"],
            "silent": true
        },
        {
            "before": ["<leader>", "f"],
            "commands": ["editor.action.formatDocument"],
            "silent": true
        },
        {
            "before": ["<leader>", "d", "s"],
            "commands": [":nohl"],
            "silent": true
        },
        {
            "before": ["<leader>", "d", "a"],
            "commands": ["workbench.debug.viewlet.action.removeAllBreakpoints"],
            "silent": true
        },
        {
            "before": ["<leader>", "t", "a"],
            "commands": ["workbench.action.toggleActivityBarVisibility"],
            "silent": true
        },
        {
            "before": ["<leader>", "s", "l"],
            "commands": ["workbench.view.extension.sln_explorer"],
            "silent": true
        },
        {
            "before": ["<leader>", "t", "l"],
            "commands": ["workbench.view.extension.todo-tree-container"],
            "silent": true
        },
        {
            "before": ["<leader>", "t", "m"],
            "commands": ["editor.action.toggleMinimap"],
            "silent": true
        },
        {
            "before": ["%"],
            "commands": ["extension.matchitJumpItems"],
            "silent": true
        },
        {
            "before": ["<leader>", "d", "t"],
            "commands": ["editor.action.revealDeclaration"],
            "silent": true
        },
        {
            "before": ["<C-d>"],
            "after": ["<C-d>", "z", "z"]
        },
        {
            "before": ["<C-u>"],
            "after": ["<C-u>", "z", "z"]
        },
        {
            "before": ["<leader>", "c", "a"],
            "commands": ["workbench.action.closeEditorsInGroup"],
            "silent": true
        },
        {
            "before": ["<leader>", "c", "e"],
            "commands": ["workbench.action.closeOtherEditors"],
            "silent": true
        },
        {
            "before": ["<C-p>"],
            "commands": ["workbench.action.quickOpen"],
            "silent": true
        },
        {
            "before": ["<leader>", "d", "n"],
            "commands": ["notifications.clearAll"],
            "silent": true
        } //
    ],
    "vim.commandLineModeKeyBindings": [
        {
            "before": ["W"],
            "after": ["w"]
        },
        {
            "before": ["Q"],
            "after": ["q"]
        }
    ],
    "csharpextensions.privateMemberPrefix": "_",
    // "[csharp]": {
    //     "editor.defaultFormatter": "ms-dotnettools.csharp"
    //     // "editor.defaultFormatter": "csharpier.csharpier-vscode"
    // },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "scm.diffDecorations": "none",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "terminal.integrated.cursorBlinking": true,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.renderFinalNewline": "off",
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "keyboard.dispatch": "keyCode",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "breadcrumbs.enabled": false,
    "editor.bracketPairColorization.enabled": false,
    "csharpextensions.useThisForCtorAssignments": false,
    "html.autoCreateQuotes": false,
    "csharpextensions.useFileScopedNamespace": true,
    "prettier.tabWidth": 4,
    "editor.inlineSuggest.enabled": false,
    "vssolution.netcoreIgnore": ["node_modules", "obj", ".ds_store", ".vs"],
    "editor.insertSpaces": false,
    "scm.diffDecorationsGutterPattern": {
        "added": true
    },
    "editor.lineNumbers": "relative",
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "window.customMenuBarAltFocus": false,
    "window.enableMenuBarMnemonics": false,
    "window.menuBarVisibility": "hidden",
    "settings.cycle": [
        {
            "id": "toggleMenuBar",
            "values": [
                { "window.menuBarVisibility": "hidden" },
                { "window.menuBarVisibility": "visible" }
            ]
        }
    ],
    "errorLens.scrollbarHackEnabled": true,
    "terminal.integrated.inheritEnv": true,
    // "gdscript_formatter.line_size": 80,
    // "godot_tools.gdscript_lsp_server_port": 6005,
    "errorLens.onSave": true,
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.iconTheme": "material-icon-theme",
    "editor.occurrencesHighlight": "off",
    "extensions.ignoreRecommendations": true,
    "editor.cursorBlinking": "solid",
    "editor.minimap.enabled": false,
    "window.zoomLevel": 1,
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "editor.scrollbar.verticalScrollbarSize": 0,
    "editor.stickyScroll.enabled": false,
    "editor.stickyScroll.maxLineCount": 1,

    // region monolisa
    "editor.fontFamily": "MonoLisa",
    "editor.fontLigatures": "'liga' on, 'ss01' on,'ss02' on, 'ss03' on, 'ss05' on, 'ss06' on, 'ss07' on, 'ss16'",
    // "editor.fontFamily": "Berkeley Mono",
    // "editor.fontLigatures": false,
    "editor.fontSize": 17,
    "editor.suggestFontSize": 17,
    "terminal.integrated.fontSize": 17,
    "editor.wordWrap": "on",
    "editor.smoothScrolling": false,
    "editor.suggestSelection": "first",
    "workbench.activityBar.location": "hidden",
    "workbench.colorTheme": "Monokai Pro (Filter Octagon)"
    // endregion  monolisa
}
